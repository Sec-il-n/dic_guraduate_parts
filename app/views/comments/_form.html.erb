<div class="ht-tm-codeblock">
  <%= form_with(model: [suggest, comment], url: suggest_comments_path(suggest))do |f| %>
    <div class="form-group form-controrl">
      <%= f.label :text %><br>
      <%= f.text_area :text, class:'field' %>
      <div class="input-group-append">
        <%= f.submit class: "btn btn-outline-info btn-sm float-right" %>
      </div>
    </div>
  <% end %>
</div>
